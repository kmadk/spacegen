{
  "name": "@figma-backend/generator",
  "version": "0.1.0",
  "description": "Transform Figma designs into live backends - AI-powered with database schema inference, API endpoints, and deployment",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "figma-backend": "dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsx watch src/index.ts",
    "clean": "rimraf dist coverage test-results",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .js --fix || echo 'No JS files to lint - TypeScript handled by compiler'",
    "format": "prettier --write src/**/*.{ts,js,json}",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "prepublishOnly": "npm run clean && npm run build && npm run test:run"
  },
  "keywords": [
    "figma",
    "backend-generator",
    "ai-powered",
    "database-schema",
    "api-endpoints",
    "vercel",
    "supabase",
    "gpt-5",
    "design-to-code"
  ],
  "author": "Figma Backend Generator Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kmadk/spacegen.git"
  },
  "homepage": "https://github.com/kmadk/spacegen#readme",
  "bugs": {
    "url": "https://github.com/kmadk/spacegen/issues"
  },
  "dependencies": {
    "@figma-backend/figma-bridge": "workspace:*",
    "@figma-backend/infrastructure": "workspace:*",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.0",
    "openai": "^4.0.0",
    "postgres": "^3.4.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/node": "^20.11.16",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "dotenv": "^17.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "@supabase/supabase-js": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "@supabase/supabase-js": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
