{
  "name": "@fir/fullstack-generator",
  "version": "0.1.0",
  "description": "Generate full-stack applications from spatial elements with database schemas and APIs",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@fir/spatial-runtime": "workspace:*",
    "@fir/figma-bridge": "workspace:*"
  },
  "devDependencies": {
    "jsdom": "^23.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.2.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "fullstack",
    "database",
    "api",
    "deployment",
    "spatial",
    "generator"
  ]
}